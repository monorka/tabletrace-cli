#!/usr/bin/env node

const { spawn } = require('child_process');
const path = require('path');

const ext = process.platform === 'win32' ? '.exe' : '';
const binary = path.join(__dirname, `tabletrace-bin${ext}`);

const child = spawn(binary, process.argv.slice(2), {
  stdio: 'inherit',
});

child.on('exit', (code) => {
  process.exit(code);
});
